let fibonacciHelper: Number a => a -> a -> Int -> a =
    lambda prev. lambda current. lambda n.
        if n > 0 then
            let a = print current in
                fibonacciHelper current (prev + current) (n - 1)
        else
            let a = print current in
                current

let fibonacci = lambda x.
    fibonacciHelper 0 1 x

let main = fibonacci 20
